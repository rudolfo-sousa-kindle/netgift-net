').html(quantidade);\\n        \\n        valorUnitario = $(this).parents('.info-itens').find('.numero-unitario').html().replace('R$&nbsp;', '');\\n        valorUnitario = parseFloat(valorUnitario.replace(',', '.'));\\n        \\n        value = 0;\\n        if(width > 600) {\\n            valorFinal = $(this).parents('.item').find('.numero-final.desk').html().replace('R$', '').replace('R$&nbsp;', '').replace(',', '.');\\n            valorFinal = valorFinal.replace('.', ',').replace('R$', '').replace('&nbsp;', '');\\n            valorFinal = parseFloat(valorFinal.replace(',', '.'));\\n            valorFinal = valorFinal + valorUnitario;\\n            \\n            $(this).parents('.item').find('.numero-final.desk').html(formatRS(valorFinal));\\n\\n            $(arrayValorDesk).map(function(index, item) {\\n                item = $(item).html().replace('&nbsp;', '').replace('R$', '').replace('.', '');\\n                item = parseFloat(item.replace(',', '.'));\\n                value = value + item;\\n                return value;\\n            })\\n            \\n            $('.numero-total').html(formatRS(value));\\n        } else {\\n            valorFinal = $(this).parents('.item').find('.numero-final.mobile').html().replace('R$', '').replace('R$&nbsp;', '').replace(',', '.');\\n            valorFinal = valorFinal.replace('.', ',').replace('R$', '').replace('&nbsp;', '');\\n            valorFinal = parseFloat(valorFinal.replace(',', '.'));\\n            valorFinal = valorFinal + valorUnitario;\\n\\n            $(this).parents('.item').find('.numero-final.mobile').html(formatRS(valorFinal));\\n\\n            $(arrayValorMobile).map(function(index, item) {\\n                item = $(item).html().replace('&nbsp;', '').replace('R$', '').replace('.', '');\\n                item = parseFloat(item.replace(',', '.'));\\n                value = value + item;\\n                return value;\\n            });\\n\\n            $('.price-gift-cart.numero-total').html(forma